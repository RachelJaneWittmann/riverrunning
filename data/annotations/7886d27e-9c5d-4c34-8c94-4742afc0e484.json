    },
    {
      "start_time": 339,
      "end_time": 366,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Long Shot proscenium style shot"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Cinematography",
          "category": "Film Analysis"
        },
        {
          "tag": "Film Analysis",
